<h1>Edit <%= %></h1>


<%= form_for(@user) do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %>

	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :password %>
	<%= f.text_field :password %>

	<%= f.label :password_confirmation %>
	<%= f.text_field :password_confirmation %>

	<%= submit_tag("Edit my acount") %>

<% end %>